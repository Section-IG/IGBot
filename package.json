{
	"name": "iesnbot",
	"version": "0.0.0",
	"description": "Henallux's unofficial Discord bot built with DiscordJS",
	"main": "./index.ts",
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc -w",
		"lint": "eslint --ext .ts ./",
		"lint:fix": "eslint --ext .ts ./ --fix",
		"prepare": "is-ci || husky install",
		"test": "echo \"Error: no test specified\"",
		"check": "yarn build && yarn lint && yarn test && npx lint-staged",
		"cm": "cz",
		"changelog": "standard-version",
		"prettier": "prettier --write *.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/section-ig/iesnbot.git"
	},
	"keywords": [
		"bot",
		"discordjs",
		"discordjs-bot",
		"discordbot",
		"henallux",
		"iesn"
	],
	"author": "HunteRoi",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/section-ig/iesnbot/issues"
	},
	"homepage": "https://github.com/section-ig/iesnbot#readme",
	"dependencies": {
		"@hunteroi/discord-mailbox": "^2.1.0",
		"@hunteroi/discord-selfrole": "^2.0.0",
		"@hunteroi/discord-server-generator": "^1.0.0",
		"@hunteroi/discord-temp-channels": "^2.1.0",
		"@hunteroi/discord-verification": "^1.0.1",
		"cron": "^1.8.2",
		"discord.js": "^13.6.0",
		"eventemitter3": "^4.0.7",
		"express": "^4.17.2",
		"express-promise-router": "^4.1.1",
		"module-alias": "^2.2.2",
		"node-fetch": "^3.2.0",
		"pg": "^8.7.1",
		"prisma": "^3.8.1",
		"require-reload": "^0.2.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@types/cron": "^1.7.3",
		"@types/express": "^4.17.13",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^17.0.10",
		"@types/node-fetch": "^3.0.3",
		"@types/pg": "^8.6.4",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"discord-api-types": "^0.26.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"husky": "^7.0.4",
		"is-ci": "^3.0.1",
		"lint-staged": "^12.3.1",
		"prettier": "^2.5.1",
		"standard-version": "^9.3.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.5",
		"yarn": "^1.22.17"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.20.3"
	},
	"lint-staged": {
		"*.ts": "eslint --cache --fix"
	},
	"husky": {
		"hooks": {
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
		}
	},
	"_moduleAliases": {
		"@config": "config.json",
		"@package": "package.json"
	}
}
