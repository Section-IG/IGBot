{
  "name": "iesnbot",
  "version": "0.0.0",
  "description": "IESN's unofficial Discord bot built with DiscordJS",
  "main": "./src/index.ts",
  "scripts": {
    "tsc": "tsc",
    "cm": "cz",
    "prepare": "is-ci || husky install",
    "changelog": "standard-version",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write src/**/*.ts",
    "test": "npm run lint",
    "test:typescript": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iesn-ig/iesnbot.git"
  },
  "keywords": [
    "bot",
    "discordjs",
    "discordjs-bot",
    "discordbot",
    "henallux",
    "iesn"
  ],
  "author": "HunteRoi",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/iesn-ig/iesnbot/issues"
  },
  "homepage": "https://github.com/iesn-ig/iesnbot#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.17.4",
    "npm": ">=7.20.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
